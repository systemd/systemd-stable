[Match]
Name=veth99

[Network]
IPv6PrivacyExtensions=yes
IPv6AcceptRA=no
DHCP=ipv6
DHCPPrefixDelegation=yes

[DHCPv6]
WithoutRA=solicit
UseAddress=no

[DHCPPrefixDelegation]
UplinkInterface=:self
SubnetId=10
Announce=no
Token=eui64
Token=::1a:2b:3c:4d
Assign=no
